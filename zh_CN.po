msgid ""
"```toml\n"
"[package]\n"
"name = \"hello_world\"\n"
"version = \"0.1.0\"\n"
"authors = [\"Your Name <you@example.com>\"]\n"
"edition = \"2018\"\n"
"\n"
"[dependencies]\n"
"time = \"0.1.12\"\n"
